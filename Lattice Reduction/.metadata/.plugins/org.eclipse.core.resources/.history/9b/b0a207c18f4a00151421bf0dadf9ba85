#include <stdio.h>
#include <stdlib.h>
#include<gsl/gsl_vector.h>
int main(void) {
	gsl_vector *v=gsl_vector_calloc(8);
	int count;
	for(count=0; count<8;count++){
		gsl_vector_set(v,count,count);
	}
	puts("!!!Hello World!!!"); /* prints !!!Hello World!!! */
	return EXIT_SUCCESS;
}
