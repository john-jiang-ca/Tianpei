function [ value ] = pdfSub( k1, k2, canji, rho )
% This subroutine calculate the sub function of pdf of orthogonality
% measure
% 
Cvalue=1;
L=length(k1);  %Nt-1
for count=1:L
    Cvalue=Cvalue*((-1)^(candidate(count))*nchoosek(k1(count)-1, candidate(count))...
        /(beta(k1(count), k2(count))));
end
value=0;
for count1=1:L
    prodTmp=1;
    for count2=1:L
        if(count2==count1)
            continue;
        end
        prodTmp=prodTmp*()
    end
end

end

